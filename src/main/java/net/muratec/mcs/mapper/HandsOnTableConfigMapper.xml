  <!-- アイコン色情報を取得する -->
  <select id="selectIconInfo" resultType="net.muratec.mcs.model.IconInfo">
    select
      HOTC.KEY as POSITION,
      HOTC.VALUE1 as TEXT,
      A.AMHS_ID,
      A.AMHS_TYPE,
      HOTC.VALUE2 as ICON_TYPE,
      HOTC.VALUE3 as ICON_STATE,
      AST.AMHS_L_STATE,
      AST.CONTROL_STATE,
      AST.AVAILABLE,
      AST.ALARM_STATE,
      AST.COMM_STATE,
      AST.SYSTEM_STATE,
      (select count(*) from PORT_STATE where PORT_ID in
        (select PORT_ID from PORT where AMHS_ID = A.AMHS_ID) and PORT_AVAIL = 0) as NOT_AVAILABLE_NUM,
      (select count(*) from PORT_STATE where PORT_ID in
        (select PORT_ID from PORT where AMHS_ID = A.AMHS_ID) and PORT_AVAIL = 1) as AVAILABLE_NUM,
      (select ZONE_UTILITY from ZONE_RLT where
        KEY = A.AMHS_ID and ZONE_ID = 'STORAGE') as ZONE_UTILITY,
      (select VALUE from MCS_CONSTS where KEY =
        '${@net.muratec.mcs.common.ComConst$SystemParameter$Key@OCCUPIED_RATE_CAUTION_LEVEL}') as OCCUPIED_CAUTION,
      (select VALUE from MCS_CONSTS where KEY =
        '${@net.muratec.mcs.common.ComConst$SystemParameter$Key@OCCUPIED_RATE_ALERT_LEVEL}') as OCCUPIED_ALERT
    from
      HANDS_ON_TABLE_CONFIG HOTC
      left outer join AMHS A on HOTC.VALUE1 = A.AMHS_ID
      left outer join AMHS_STATE AST on HOTC.VALUE1 = AST.AMHS_ID
    where
      HOTC.SECTION = '${@net.muratec.mcs.common.ComConst$HandsOnTableConfig@ICON}'
  </select>
