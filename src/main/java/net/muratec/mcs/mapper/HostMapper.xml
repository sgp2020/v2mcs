   <select id="getCount" parameterType="net.muratec.mcs.entity.info.ReqGetStockerInfoEntity" resultType="java.lang.Integer"> 
	   SELECT  
		  count(*)
	   FROM(
		  	SELECT 
		  		   0 num, 
				    host_ip, 
					host_name, 
					comm_state, 
					( CASE WHEN conn_time IS NULL THEN null 
						 ELSE ROUND( ( sysdate - TO_DATE( SUBSTR( conn_time, 1, 14 ), 'YYYYMMDDHH24MISS' ) )*24*3600 ) 
					END ) passage_time  
			FROM   host
	    	<include refid="hostWhere" />
			ORDER BY host_ip, host_name
		)
  </select>
  <!-- レコード取得範囲決定部 -->
  <sql id="selectRecordWhere">
    <where>
      <if test="fromRecordNum != null and toRecordNum != null">
       rn between #{fromRecordNum,jdbcType=DECIMAL} and #{toRecordNum,jdbcType=DECIMAL}
      </if>
    </where>
  </sql>
