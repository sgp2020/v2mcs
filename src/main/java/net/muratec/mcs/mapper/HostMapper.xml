  <sql id="hostWhere">
    <where>
      <if test="hostName != null and hostName !='' ">
        host_name = #{hostName}
      </if>
      <if test="commState != null and commState !='' ">
        and comm_state = #{commState}
      </if>
    </where>
  </sql>
  <select id="selectHostCommInfoList" parameterType="net.muratec.mcs.entity.info.ReqGetHostCommInfoEntity" resultMap="BaseResultMap">
	SELECT 	
	  	num,
		host_ip, 
		host_name, 
		comm_state, 
		passage_time 
	FROM(
		   SELECT 
		  		    row_number() over(order by host_ip, host_name) rn,<!-- Paging -->
		  		    0 num, 
				    host_ip, 
					host_name, 
					comm_state, 
					( CASE WHEN conn_time IS NULL THEN null 
						 ELSE ROUND( ( sysdate - TO_DATE( SUBSTR( conn_time, 1, 14 ), 'YYYYMMDDHH24MISS' ) )*24*3600 ) 
					END ) passage_time  
			FROM   host
	    	<include refid="hostWhere" />
			ORDER BY host_ip, host_name
		)
		<include refid="selectRecordWhere" />
    </select>
    
