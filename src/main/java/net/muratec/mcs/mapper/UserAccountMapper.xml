  <!-- ユーザ管理マスタメンテ一覧表示 データ取得用 -->
  <select id="selectList" parameterType="net.muratec.mcs.entity.common.AjaxDataTablesReqBaseEntity" resultMap="BaseResultMap">
    select
      USER_ID,
      USER_NAME,
      DESCRIPTION,
      GUI_GROUP_ID,
      LAST_LOGIN
    from (
      select
        ROW_NUMBER() over(order by USER_ID asc) rn,
        USER_ID,
        USER_NAME,
        DESCRIPTION,
        GUI_GROUP_ID,
        LAST_LOGIN
      from
        USER_ACCOUNT
      order by
        USER_ID asc
    )
