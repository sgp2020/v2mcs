  <!-- ユーザ登録 USER_IDに重複しない最小番号を採番しユーザを登録する. -->
  <insert id="insertByNonOverlapId" parameterType="net.muratec.mcs.model.UserAccount">
    insert into USER_ACCOUNT (USER_ID, USER_NAME, DESCRIPTION,
      GUI_GROUP_ID, PASSWORD, PASS_ERROR_COUNT,
      ACCOUNT_STATE, LAST_LOGIN)
    values (
      (select
        min(d.count)
      from
        (select LEVEL as COUNT from DUAL connect by LEVEL &lt; 10000) d
      left outer join
        USER_ACCOUNT u
      on
        d.COUNT = u.USER_ID
      where
        USER_ID is null
      ),
