  <select id="getCount" parameterType="net.muratec.mcs.entity.info.ReqGetOhbInfoListEntity" resultType="java.lang.Integer"> 
   SELECT  
	  count(*)
   FROM (
	       SELECT
	          ohb_id,  
		      TRUNC( ( CASE total_shelves WHEN 0 THEN 0  
	                   ELSE ( total_shelves - empty_shelves ) / total_shelves * 100  
			           END ), 0 )   occupancy,  
		      ( total_shelves - empty_shelves ) used ,  
		      total_shelves  total
		   FROM   OHB  
		   ORDER BY ohb_id
      )
  </select>
  
