  <!--  Ohb情報一覧を取得する。 -->
  <select id="selectOhbInfoList" parameterType="net.muratec.mcs.entity.info.ReqGetOhbInfoListEntity" resultMap="SearchResultMap">
   SELECT  
	   ohb_id,  
	   occupancy,  
	   used,  
	   total
   FROM (
	       SELECT
	          row_number() over(order by ohb_id asc) rn,
	          ohb_id,  
		      TRUNC( ( CASE total_shelves WHEN 0 THEN 0  
	                   ELSE ( total_shelves - empty_shelves ) / total_shelves * 100  
			           END ), 0 )   occupancy,  
		      ( total_shelves - empty_shelves ) used ,  
		      total_shelves  total
		   FROM   OHB  
		   ORDER BY ohb_id
