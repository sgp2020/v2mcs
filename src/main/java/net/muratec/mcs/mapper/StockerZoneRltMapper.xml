  <!-- 以下独自SQL -->
   <select id="getCount" parameterType="net.muratec.mcs.entity.info.ReqGetStockerInfoEntity" resultType="java.lang.Integer"> 
	   SELECT  
		  count(*)
	   FROM(
		  	SELECT 
		  		   0 num, 
				   szr.tsc_id, 
				   szr.zone_id, 
				   TRUNC( ( CASE szr.high_water_mark           
								WHEN 0 THEN 0  
								ELSE ((szr.high_water_mark - szr.l_empty_shelves)/szr.high_water_mark)*100  
							END ), 0 ) L_OCCUPANCY, 
				   TRUNC( ( CASE szr.total_shelves               
								WHEN 0 THEN 0  
								ELSE ((szr.total_shelves - szr.empty_shelves)/szr.total_shelves)*100  
							END ), 0 ) OCCUPANCY, 
				   (szr.total_shelves - szr.empty_shelves) USED_CELL, 
				   szr.total_shelves, 
				   szr.low_water_mark, 
				   szr.high_water_mark, 
				   tsc.tsc_abbreviation 
			FROM   stocker_zone_rlt szr 
			LEFT OUTER JOIN tsc ON tsc.tsc_id = szr.tsc_id 
	    	<include refid="tscIdWhere" />
			ORDER BY tsc.tsc_abbreviation, szr.zone_id
		)
  </select>
