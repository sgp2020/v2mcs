  <select id="selectStockerInfoList" parameterType="net.muratec.mcs.entity.info.ReqGetStockerInfoEntity" resultMap="BaseResultMap">
	SELECT 	
	  	num,
	  	tsc_id, 
		zone_id,
		l_occupancy,
		occupancy, 
		used_cell, 
		total_shelves, 
		low_water_mark, 
		high_water_mark, 
		tsc_abbreviation 
	FROM(
		  	SELECT 
		  		   row_number() over(order by tsc.tsc_abbreviation, szr.zone_id) rn,<!-- Paging -->
		  		   0 num, 
				   szr.tsc_id, 
				   szr.zone_id, 
				   TRUNC( ( CASE szr.high_water_mark           
								WHEN 0 THEN 0  
								ELSE ((szr.high_water_mark - szr.l_empty_shelves)/szr.high_water_mark)*100  
							END ), 0 ) L_OCCUPANCY, 
				   TRUNC( ( CASE szr.total_shelves               
								WHEN 0 THEN 0  
								ELSE ((szr.total_shelves - szr.empty_shelves)/szr.total_shelves)*100  
							END ), 0 ) OCCUPANCY, 
				   (szr.total_shelves - szr.empty_shelves) USED_CELL, 
				   szr.total_shelves, 
				   szr.low_water_mark, 
				   szr.high_water_mark, 
				   tsc.tsc_abbreviation 
			FROM   stocker_zone_rlt szr 
			LEFT OUTER JOIN tsc ON tsc.tsc_id = szr.tsc_id 
			<!-- <if test="tscId != null || tscId !='' " >
			WHERE szr.tsc_id = #{tscId,jdbcType=DECIMAL}
	    	</if> -->
	    	<include refid="tscIdWhere" />
			ORDER BY tsc.tsc_abbreviation, szr.zone_id
		)
		<include refid="selectRecordWhere" />
    </select>
